                    "appSettings": [
                        {
                            "name": "AzureWebJobsStorage",
                            "value": "[concat('DefaultEndpointsProtocol=https;AccountName=', parameters('storageAccounts_storagecovidrtw_name'), ';EndpointSuffix=', environment().suffixes.storage, ';AccountKey=',listKeys(resourceId('Microsoft.Storage/storageAccounts', parameters('storageAccounts_storagecovidrtw_name')), '2019-06-01').keys[0].value)]"
                        },
                        {
                            "name": "WEBSITE_CONTENTAZUREFILECONNECTIONSTRING",
                            "value": "[concat('DefaultEndpointsProtocol=https;AccountName=', parameters('storageAccounts_storagecovidrtw_name'), ';EndpointSuffix=', environment().suffixes.storage, ';AccountKey=',listKeys(resourceId('Microsoft.Storage/storageAccounts', parameters('storageAccounts_storagecovidrtw_name')), '2019-06-01').keys[0].value)]"
                        },
                        {
                            "name": "WEBSITE_CONTENTSHARE",
                            "value": "covidrtw"
                        },
                        {
                            "name": "SqlConnectionString",
                            "value": "[concat('Server=tcp:',parameters('servers_sql_covidrtw_name'),'.database.windows.net,1433;Initial Catalog=covidrtw-db;Persist Security Info=False;User ID=',parameters('sql_admin'),';Password=',parameters('sql_password'),';MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;')]"
                        },
                        {
                            "name": "FUNCTIONS_EXTENSION_VERSION",
                            "value": "~2"
                        },
                        {
                            "name": "WEBSITE_NODE_DEFAULT_VERSION",
                            "value": "~10"
                        },
                        {
                            "name": "FUNCTIONS_WORKER_RUNTIME",
                            "value": "dotnet"
                        },
                        {
                            "name": "WEBSITE_VNET_ROUTE_ALL",
                            "value": "1"
                        },
                        {
                            "name": "WEBSITE_DNS_SERVER",
                            "value": "168.63.129.16"
                        },
                        {
                            "name": "SCM_DO_BUILD_DURING_DEPLOYMENT",
                            "value": true
                        }
                    ]
                },
                "httpsOnly": true
            }
        },
        {
            "type": "Microsoft.Web/sites/networkConfig",
            "name": "[concat(parameters('sites_fx_covidrtw_name'),'/VirtualNetwork')]",
            "apiVersion": "2016-08-01",
            "dependsOn": [
                "[resourceId('Microsoft.Web/sites', parameters('sites_fx_covidrtw_name'))]",
