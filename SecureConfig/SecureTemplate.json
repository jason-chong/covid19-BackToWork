            ],
            "properties": { "subnetResourceId": "[resourceId('Microsoft.Network/virtualNetworks/subnets', parameters('virtualNetworks_vnet_covidrtw_name'), 'appServiceSubnet')]" }
        },
        {
            "type": "Microsoft.Web/sites/hostNameBindings",
            "apiVersion": "2018-11-01",
            "name": "[concat(parameters('sites_fx_covidrtw_name'), '/', parameters('sites_fx_covidrtw_name'), '.azurewebsites.net')]",
            "location": "[parameters('location')]",
            "dependsOn": [
                "[resourceId('Microsoft.Web/sites', parameters('sites_fx_covidrtw_name'))]"
            ],
            "properties": {
                "siteName": "[parameters('sites_fx_covidrtw_name')]",
                "hostNameType": "Verified"
            }
        },
